{
    "manifest_version": 2,
    "name": "Don't merge my WIPs!",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*/pull/*"
            ],
            "js": ["jquery-2.2.0.min.js", "content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}
